{
    "name": "Easy Skript",
    "version": "1.0",
    "description": "A syntax highlighting autocompleting code editor to replace the default Minehut Skript editor.",
    "manifest_version": 2,
    "icons": {
    	"16": "icon/icon16.png",
    	"32": "icon/icon32.png",
        "48": "icon/icon48.png",
        "128": "icon/icon128.png"
    },
    "browser_action": {
        "default_popup": "popup/popup.html"
    },
    "content_scripts": [
        {
            "matches": ["https://minehut.com/dashboard/*"],
            "js": ["content.js"]
        }
    ]
}